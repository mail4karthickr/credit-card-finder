{
  "name": "credit_card_finder_ui",
  "version": "1.0.0",
  "description": "Wells Fargo Credit Card Finder UI Components",
  "scripts": {
    "build": "esbuild src/CardList/CardList.jsx --bundle --format=esm --outfile=dist/card-list.js --loader:.png=dataurl --loader:.svg=dataurl && esbuild src/CardCompare/CardCompare.jsx --bundle --format=esm --outfile=dist/card-compare.js --loader:.png=dataurl --loader:.svg=dataurl",
    "watch": "esbuild src/CardList/CardList.jsx --bundle --format=esm --outfile=dist/card-list.js --watch --loader:.png=dataurl --loader:.svg=dataurl",
    "dev": "npm run build && npx http-server -p 8000 -c-1 -o",
    "test": "npm run build && npm run dev",
    "start": "open test_build.html"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.1.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "esbuild": "^0.19.0",
    "http-server": "^14.1.1",
    "typescript": "^5.0.0"
  }
}
